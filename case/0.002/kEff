FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0.002";
    object      kEff;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 1 -3 -1 0 0 0];

internalField   uniform 10;

boundaryField
{
    bottom
    {
        type            calculated;
        value           uniform 10;
    }
    top
    {
        type            calculated;
        value           uniform 10;
    }
    left
    {
        type            calculated;
        value           uniform 10;
    }
    right
    {
        type            calculated;
        value           uniform 10;
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
}


